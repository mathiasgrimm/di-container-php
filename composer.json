{
  "name": "mathiasgrimm/di-container-php",
  "type": "library",
  "description": ":package_description",
  "keywords": [
    "mathiasgrimm",
    "di-container-php",
    "dependency injection",
    "ioc",
    "container",
    "autowire",
    "wire"
  ],
  "homepage": "https://github.com/mathiasgrimm/di-container-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathias Grimm",
      "email": "mathiasgrimm@gmail.com",
      "homepage": "https://github.com/mathiasgrimm",
      "role": "Developer"
    }
  ],

  "require": {
    "php" : "~5.6|~7.0",
    "mathiasgrimm/arraypath": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit" : ">=5.4.3",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "MathiasGrimm\\DiContainer\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MathiasGrimm\\DiContainerTest\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
